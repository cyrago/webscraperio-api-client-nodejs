{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "module": "commonjs",
    "target": "ES2017",
    "allowJs": true,
    "lib": ["es2017", "dom", "esnext.asynciterable"],
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
